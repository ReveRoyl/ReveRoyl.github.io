<% if (theme.socialLink.github) { %>
    <a href="https://github.com/<%= theme.socialLink.github %>" class="tooltipped" target="_blank" data-tooltip="visit my GitHub" data-position="top" data-delay="50">
        <i class="fa fa-github"></i>
    </a>
<% } %>

<% if (theme.socialLink.email) { %>
    <a href="mailto:<%= theme.socialLink.email %>" class="tooltipped" target="_blank" data-tooltip="email me" data-position="top" data-delay="50">
        <i class="fa fa-envelope-open"></i>
    </a>
<% } %>



<% if (theme.socialLink.facebook) { %>
    <a href="https://facebook.com/<%= theme.socialLink.facebook %>" class="tooltipped" target="_blank" data-tooltip="visit my facebook" data-position="top" data-delay="50">
        <i class="fa fa-facebook"></i>
    </a>
<% } %>

<% if (theme.socialLink.linkedin) { %>
    <a href="https://linkedin.com/<%= theme.socialLink.linkedin %>" class="tooltipped" target="_blank" data-tooltip="visit my linkedin" data-position="top" data-delay="50">
        <i class="fa fa-linkedin"></i>
    </a>
<% } %>


<% if (config.feed && config.feed.path == 'atom.xml') { %>
    <a href="<%- url_for('/atom.xml') %>" class="tooltipped" target="_blank" data-tooltip="RSS subscribe" data-position="top" data-delay="50">
        <i class="fa fa-rss"></i>
    </a>
<% } %>